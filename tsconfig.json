{
	"compilerOptions": {
		"allowJs": true,
		"baseUrl": "./",
		"forceConsistentCasingInFileNames": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"lib": ["dom", "dom.iterable", "esnext"],
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"skipLibCheck": true,
		"strict": false,
		"target": "es5"
	},
	"exclude": ["node_modules"],
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/CustomPage/index.jsx", "graphql/cart.ts", "pages/account/index.jsx", "components/Alert/alert.stories.tsx", "main.js"]
}
